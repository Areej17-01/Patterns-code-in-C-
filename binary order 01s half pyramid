

int main()
{
	int row;
	cin >> row;
	for (int i = 1; i <= row; i++) {//2

		for (int j = 1; j <= i; j++) { //2
			if (i % 2 == 0) {
				if (j % 2 != 0) {
					cout << 0;
				}
				else {
					cout << 1;
				}
			}
			else {
				if (j % 2 == 0) {
					cout << 0;
				}

				else {
					cout << 1;
				}
			}
			
			

		}
		cout << endl;
	}
}
